{
  "name": "level-up",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:all-sports": "ng serve -a all-sports-products -p 4500",
    "start:youtube": "ng serve -a youtube-api -p 4200",
    "start:twitter": "ng serve -a twitter-api -p 4201",
    "start:starwars": "ng serve -a starwars-api -p 4202",
    "start:poke": "ng serve -a poke-api -p 4203",
    "start:googlebooks": "ng serve -a googlebooks-api -p 4204",
    "start:spotify": "ng serve -a spotify-api -p 4205",
    "start:itunes": "ng serve -a itunes-api -p 4206",
    "start:venturbook": "ng serve -a venturbook -p 4207",
    "build": "ng build",
    "deploy:youtube": "ng build -a youtube-api && cp ./dist/apps/youtube-api/index.html ./dist/apps/youtube-api/200.html && surge ./dist/apps/youtube-api https://youtube-api-app.surge.sh",
    "deploy:twitter": "ng build -a twitter-api && cp ./dist/apps/twitter-api/index.html ./dist/apps/twitter-api/200.html && surge ./dist/apps/twitter-api https://twitter-app.surge.sh",
    "deploy:starwars": "ng build -a starwars-api && cp ./dist/apps/starwars-api/index.html ./dist/apps/starwars-api/200.html && surge ./dist/apps/starwars-api https://starwars-api-app.surge.sh",
    "deploy:poke": "ng build -a poke-api && cp ./dist/apps/poke-api/index.html ./dist/apps/poke-api/200.html && surge ./dist/apps/poke-api https://poke-app.surge.sh",
    "deploy:venturbook": "ng build -a venturbook && cp ./dist/apps/venturbook/index.html ./dist/apps/venturbook/200.html && surge ./dist/apps/venturbook https://venturbook.surge.sh",
    "test": "ng test",
    "lint": "./node_modules/.bin/nx lint && ng lint",
    "e2e": "ng e2e",
    "affected:apps": "./node_modules/.bin/nx affected:apps",
    "affected:build": "./node_modules/.bin/nx affected:build",
    "affected:e2e": "./node_modules/.bin/nx affected:e2e",
    "affected:dep-graph": "./node_modules/.bin/nx affected:dep-graph",
    "format": "./node_modules/.bin/nx format:write",
    "format:write": "./node_modules/.bin/nx format:write",
    "format:check": "./node_modules/.bin/nx format:check",
    "update": "./node_modules/.bin/nx update",
    "update:check": "./node_modules/.bin/nx update:check",
    "update:skip": "./node_modules/.bin/nx update:skip",
    "workspace-schematic": "./node_modules/.bin/nx workspace-schematic",
    "dep-graph": "./node_modules/.bin/nx dep-graph",
    "postinstall": "./node_modules/.bin/nx postinstall",
    "help": "./node_modules/.bin/nx help"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.2.7",
    "@angular/cdk": "5.2.4",
    "@angular/common": "5.2.7",
    "@angular/compiler": "5.2.7",
    "@angular/core": "5.2.7",
    "@angular/forms": "5.2.7",
    "@angular/material": "5.2.4",
    "@angular/platform-browser": "5.2.7",
    "@angular/platform-browser-dynamic": "5.2.7",
    "@angular/router": "5.2.7",
    "@ngrx/effects": "5.2.0",
    "@ngrx/router-store": "5.0.1",
    "@ngrx/store": "5.2.0",
    "@ngrx/store-devtools": "5.2.0",
    "@nrwl/nx": "1.0.3",
    "core-js": "^2.4.1",
    "ng2-swapi": "^1.0.0-rc.11",
    "ngrx-store-freeze": "^0.2.1",
    "rxjs": "^5.5.6",
    "viz": "^0.0.1",
    "zone.js": "^0.8.19"
  },
  "devDependencies": {
    "@angular/cli": "1.7.1",
    "@angular/compiler-cli": "5.2.7",
    "@angular/language-service": "5.2.7",
    "@ngrx/schematics": "5.2.0",
    "@nrwl/schematics": "1.0.3",
    "@types/jasmine": "~2.8.3",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "codelyzer": "^4.0.1",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "prettier": "1.10.2",
    "protractor": "~5.1.2",
    "surge": "^0.20.1",
    "ts-node": "~4.1.0",
    "tslint": "~5.9.1",
    "typescript": "2.6.2"
  }
}
